{
    "name": "quuulimited/api-php",
    "description": "The Quuu Content API for PHP installations",
    "keywords": [
        "api",
        "quuu",
        "promote"
    ],
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Quuu Limites",
            "homepage": "https://quuu.co"
        }
    ],
    "autoload": {
        "psr-4": {
            "Quuu\\": [
                "src"
            ]
        }
    },
    "require": {
        "php": ">= 7.1",
        "ext-json": "*",
        "php-http/client-common": "^1.9 || ^2.0",
        "php-http/client-implementation": "^1.0",
        "php-http/discovery": "^1.4",
        "php-http/httplug": "^1.0 || ^2.0",
        "php-http/message": "^1.7",
        "psr/http-message": "^1.0"
    },
    "require-dev": {
        "php-http/guzzle6-adapter": "^1.0 || ^2.0",
        "phpunit/phpunit": "^7.0",
        "squizlabs/php_codesniffer": "^3.1"
    },
    "config": {
        "sort-packages": true
    }
}